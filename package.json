{
  "name": "@moznion/miniq",
  "version": "0.0.1",
  "description": "A minimal in-process queue system for TypeScript/JavaScript.",
  "main": "index.js",
  "types": "index.d.ts",
  "scripts": {
    "build": "tsc",
    "test": "jest --no-cache --coverage",
    "lint": "eslint .",
    "fmt": "find -name '*.ts' -not -path '*.d.ts' -not -path './node_modules/*' | xargs prettier --write",
    "gendoc": "typedoc ./*.ts",
    "release": "npm run build && npm publish --access=public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/moznion/miniq.git"
  },
  "bugs": {
    "url": "https://github.com/moznion/miniq/issues"
  },
  "homepage": "https://github.com/moznion/miniq#readme",
  "author": {
    "name": "moznion",
    "email": "moznion@mail.moznion.net"
  },
  "license": "MIT",
  "files": [
    "index.js",
    "index.d.ts"
  ],
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^5.48.2",
    "@typescript-eslint/parser": "^5.48.2",
    "eslint": "^8.32.0",
    "jest": "^29.3.1",
    "prettier": "^2.8.3",
    "ts-jest": "^29.0.5",
    "typedoc": "^0.23.24",
    "typescript": "^5.0.0"
  }
}
